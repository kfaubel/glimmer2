# Glimmer Environment Configuration
# All environment variables must start with REACT_APP_ to be accessible in the browser

# Base URL for screen list configurations
REACT_APP_SCREEN_LIST_URL_BASE=https://glimmerstorage.blob.core.windows.net/glimmer/config/

# Development settings
REACT_APP_ENV=development

# Feature flags
REACT_APP_ENABLE_LOGGING=true
